# gazelle:prefix k8s.io/kops

load("@bazel_gazelle//:def.bzl", "gazelle")

package(default_visibility = ["//visibility:public"])

load("@io_bazel_rules_go//go:def.bzl", "go_library")

gazelle(
    name = "gazelle",
    command = "fix",
    external = "vendored",
    prefix = "k8s.io/kops",
)

go_library(
    name = "go_default_library",
    srcs = [
        "doc.go",
        "version.go",
    ],
    importpath = "k8s.io/kops",
)
